{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch (api) [5003]",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/Training.API.Plans/bin/Debug/net7.0/Training.API.Plans.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/Training.API.Plans",
      "stopAtEntry": false,
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "http://localhost:5003"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/src/Training.API.Plans/Views"
      }
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach"
    }
  ]
}
